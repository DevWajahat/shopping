@model Feedback
<div class="container-fluid px-4">
    <h2 class="mt-4 text-center text-danger text-uppercase" style="letter-spacing:4px">Are you sure you want to delete this Feedback</h2>
</div>
<div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-8">
        <ul class="list-group">

            <li class="list-group-item">Feedback  Id : @Model.Feedback_id</li>
            <li class="list-group-item">User Name : @Model.user_name</li>
            <li class="list-group-item">User Message : @Model.user_message</li>


        </ul>

        <a href="@Url.Action("deleteFeedback","Dealer",new{id=Model.Feedback_id})" class="btn btn-danger">Delete </a>
        <a href="@Url.Action("fetchfeedback","Dealer")" class="btn btn-warning">Back to list</a>

    </div>
    <div class="col-md-2">
    </div>
