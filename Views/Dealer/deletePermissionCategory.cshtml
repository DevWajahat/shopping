@model Category
<div class="container-fluid px-4">
    <h2 class="mt-4 text-center text-danger text-uppercase" style="letter-spacing:4px">Are you sure you want to delete this Category</h2>
</div>
<div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-8">
        <ul class="list-group">

            <li class="list-group-item">Customer Id : @Model.category_id</li>
            <li class="list-group-item">Customer Name : @Model.category_name</li>
        </ul>

        <a href="@Url.Action("deletecategory","Dealer",new{id=Model.category_id})" class="btn btn-danger">Delete </a>
        <a href="@Url.Action("fetchcustomer","Dealer")" class="btn btn-warning">Back to list</a>

    </div>
    <div class="col-md-2">
    </div>
